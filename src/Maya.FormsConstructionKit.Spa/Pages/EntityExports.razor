@page "/exports/{entityName}"

<PageTitle>@EntityName exports</PageTitle>

<Maya.FormsConstructionKit.Spa.UI.Components.Core.Container.ContentContainer 
    IsBusy=this.vm!.IsBusy
    IsLoading=this.vm!.IsInit
    NotifyMessage=this.NotifyMessage>
    <Content>
        <CascadingValue Name="@(nameof(Library.Contract.UI.ViewModels.Exports.IExportsViewModel))" Value="@vm">
            <Maya.FormsConstructionKit.Spa.UI.Components.Views.Exports.View />
        </CascadingValue>
    </Content>
</Maya.FormsConstructionKit.Spa.UI.Components.Core.Container.ContentContainer>